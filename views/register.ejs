<body class="text-center">
  <div class="container">
    <h1 class="display-5" style="text-align: center;"><i class="fas fa-home mr-2 text-dark"></i>rumahlaku</h1>
    <form class="form-signin" action="/register" method="POST">
      <h1 class="h3 mb-3 font-weight-normal">Register</h1>
      <label for="inputName" class="sr-only">Your name</label>
      <input type="name" id="inputName" name="nama" class="form-control" placeholder="Your name" required="">
      <label for="inputEmail" class="sr-only">Email address</label>
      <input type="email" id="inputEmail" name="email" class="form-control" placeholder="Email address" required="">
      <label for="inputPassword" class="sr-only">Password</label>
      <input type="password" id="inputPassword" name="password" class="form-control" placeholder="Password" required=""
        style="margin-bottom: 0px; margin-top: 0px;">
      <label for="inputPassword" class="sr-only">Password</label>
      <input type="password" id="inputPassword" name="repassword" class="form-control"
        placeholder="Retype your password" required="" style="margin-bottom: 0px;">
      <label for="inputNohandphone" class="sr-only">No handphone</label>
      <input type="nohandphone" id="inputNohandphone" name="nohandphone" class="form-control"
        placeholder="Your phone number" required="" style="margin-bottom: 10px; margin-top: 0px;">

      <button class="btn btn-lg btn-primary btn-block" type="submit">Register</button>
      <% if (info.length > 0) { %>
      <div class="alert alert-success" role="alert">
        <%= info %>
      </div>
      <% } %>
      <p class="mt-5 mb-3 text-muted">Â© Batch 23 Rubicamp 2020</p>
    </form>
  </div>
